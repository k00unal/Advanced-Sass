//- You can also loop through an array:

- const spaceamounts = ["5", "10", "15", "20", "25", "30", "35", "40", "45", "50", "75", "100"]
- const sides = ["top", "bottom", "left", "right"]
  each space in spaceamounts
    each side in sides
        div( class='m-'+(side.slice(0, 1)+ -space) )= 'margin' +'-' + side + '-' + space +'px'
        div( class='p-'+(side.slice(0, 1)+ -space) )= 'padding' +'-' + side + '-' + space +'px'